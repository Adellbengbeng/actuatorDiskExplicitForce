FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      VolumeForce;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            fixedValue;
        value           $internalField;
    }

    walls
    {
        type            fixedValue;
        value           $internalField;
    }

}

// ************************************************************************* //
